{
	"level": "info",
	"development": false,
	"disableCaller": true,
	"disableStacktrace": false,
	"encoding": "json",
	"encoderConfig": {
		"messageKey": "Msg",
		"levelKey": "Level",
		"timeKey": "Time",
		"nameKey": "Name",
		"callerKey": "Caller",
		"stacktraceKey": "St",
		"levelEncoder": "capital",
		"timeEncoder": "iso8601",
		"durationEncoder": "string",
